{
  "name": "postcss-smart-asset",
  "version": "1.0.4",
  "description": "PostCSS plugin to rebase or inline on url().",
  "keywords": [
    "css",
    "postcss",
    "postcss-plugin",
    "url",
    "rebase",
    "inline",
    "base64",
    "assets"
  ],
  "engines": {
    "node": ">=8.0.0",
    "yarn": ">=1.0.0",
    "npm": ">=4.0.0"
  },
  "license": "MIT",
  "repository": "https://github.com/sebastian-software/postcss-smart-asset.git",
  "author": {
    "name": "Sebastian Software",
    "email": "s.werner@sebastian-software.de",
    "url": "https://www.sebastian-software.de"
  },
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": "eslint --quiet"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "asset-hash": "^3.0.4",
    "core-js": "^3.6.5",
    "cp-file": "^9.0.0",
    "mime": "^2.4.5",
    "minimatch": "^3.0.4",
    "pify": "^5.0.0",
    "postcss": "^7.0.29"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@effective/eslint-config": "^2.6.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.0.1",
    "babel-preset-edge": "^5.5.1",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "jest-cli": "^26.0.1",
    "lint-staged": "^10.2.2",
    "postcss-import": "^12.0.1",
    "preppy": "^10.4.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "lint": "eslint '**/*.js'",
    "fix": "eslint --fix '**/*.js'",
    "format": "prettier --write '**/*.js' && npm run fix",
    "release": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major",
    "test": "jest --coverage",
    "prepare": "rimraf lib && preppy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
